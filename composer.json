{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.31",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "friendsofsymfony/rest-bundle": "^2.7",
        "friendsofsymfony/user-bundle": "~2.0",
        "jms/serializer": "^3.6.0",
        "jms/serializer-bundle": "3.6",
        "lexik/jwt-authentication-bundle": "^2.6",
        "mhujer/jms-serializer-uuid-bundle": "^4.1",
        "nelmio/api-doc-bundle": "^3.6",
        "nelmio/cors-bundle": "^2.0",
        "nesbot/carbon": "^2.35",
        "ramsey/uuid-doctrine": "^1.6",
        "sensio/framework-extra-bundle": "^5.5",
        "sonata-project/admin-bundle": "^3.66",
        "sonata-project/doctrine-orm-admin-bundle": "^3.17",
        "sonata-project/user-bundle": "^4.5",
        "stof/doctrine-extensions-bundle": "^1.4",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/orm-pack": "^2.0",
        "symfony/swiftmailer-bundle": "^3.4",
        "symfony/twig-bundle": "4.4.*",
        "symfony/yaml": "4.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "fzaninotto/faker": "^1.9",
        "sonata-project/easy-extends-bundle": "^2.5",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/debug-bundle": "*",
        "symfony/maker-bundle": "^1.17",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/profiler-pack": "*",
        "symfony/var-dumper": "*",
        "symfony/web-server-bundle": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.dist', '.env');\"",
            "@php -r \"file_exists('phpunit.xml.dist') || copy('phpunit.xml.dist', 'phpunit.xml');\""
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
